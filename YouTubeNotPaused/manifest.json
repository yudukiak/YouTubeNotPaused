{
  "manifest_version": 3,
  "name": "YouTube Not Paused",
  "version": "1.0.1",
  "description": "Video paused. Continue watching?",
  "author": "akasaka",
  "icons": {
    "16": "icons/16.png",
    "64": "icons/64.png",
    "128": "icons/128.png"
  },
  "content_scripts": [{
    "matches": [
      "https://www.youtube.com/watch?v=*"
    ],
    "js": ["js/index.js"],
    "run_at": "document_end",
    "all_frames": true
  }]
}